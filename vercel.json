{
    "version": 2,
    "builds": [
      {
        "src": "backend/api/index.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend/api/index.py"
      }
    ],
    "functions": {
      "api/index.py": {
        "runtime": "python3.9"
      }
    }
}